package com.ict.edu01.shop.vo;

public class CartVO {
	private String cart_idx, p_num, p_name, p_price, p_saleprice, p_su, p_id;

	public String getCart_idx() {
		return cart_idx;
	}

	public void setCart_idx(String cart_idx) {
		this.cart_idx = cart_idx;
	}

	public String getP_num() {
		return p_num;
	}

	public void setP_num(String p_num) {
		this.p_num = p_num;
	}

	public String getP_name() {
		return p_name;
	}

	public void setP_name(String p_name) {
		this.p_name = p_name;
	}

	public String getP_price() {
		return p_price;
	}

	public void setP_price(String p_price) {
		this.p_price = p_price;
	}

	public String getP_saleprice() {
		return p_saleprice;
	}

	public void setP_saleprice(String p_saleprice) {
		this.p_saleprice = p_saleprice;
	}

	public String getP_su() {
		return p_su;
	}

	public void setP_su(String p_su) {
		this.p_su = p_su;
	}

	public String getP_id() {
		return p_id;
	}

	public void setP_id(String p_id) {
		this.p_id = p_id;
	}
	
}
