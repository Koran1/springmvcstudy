<mapper namespace="guestbook2">
	<select id="list" resultType="gbvo">
		select * from guestbook2 order by gb2_idx
	</select>
	<select id="onelist" parameterType="String" resultType="gbvo">
		select * from guestbook2 where gb2_idx = #{gb2_idx}
	</select>
</mapper>